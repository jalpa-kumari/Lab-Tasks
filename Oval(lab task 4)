public class oval extends Shape{
	
	public double x,y;
	public String ShapeName,col;
	public oval (int x, int y){
	
		super("Oval");
	
	}
	public oval (String ShapeName)
	{

		super(ShapeName);
	}
			
	public double area() {
		return Math.PI*x*y;
		
	}
	public double perimeter()
	{
		return 2*Math.PI*x*y;
	
	}
	
	public String color()
	
	{
	return col;	
	}	
}
